{
  "name": "YoutubeEx",
  "version": "1.2.0",
  "permissions": [
    "storage"
  ],
  "manifest_version": 3,
  "description": "Youtubeに追加機能を提供する拡張機能。追加したい項目をチェックすると使用可能。",
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "all_frames": true,
    "js": [
      "initialize.js",
      "content.js"
    ]
  }],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}